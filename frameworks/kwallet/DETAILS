          MODULE=kwallet
         VERSION=5.24.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:5dbff47f16f96bc4eb664866d3cd5d3d9c2508e452f73d7617946dbd092c4d7d
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=https://projects.kde.org/projects/frameworks/kwallet
         ENTERED=20150205
         UPDATED=20160709
           SHORT="safe desktop-wide storage for passwords"

cat << EOF
Safe desktop-wide storage for passwords

This framework contains two main components:
* Interface to KWallet, the safe desktop-wide storage for passwords on KDE work 
spaces.
* The kwalletd used to safely store the passwords on KDE work spaces.
EOF
