          MODULE=kdnssd
         VERSION=6.23.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:54403a8446b39d1e5e7d34e5fd9320ce6f671288830a0030a0c3d7ed6bfc1d78
