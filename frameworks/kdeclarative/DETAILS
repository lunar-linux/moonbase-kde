          MODULE=kdeclarative
         VERSION=5.55.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:aa51a98e66dcfeb08d26cc79b230fac3eac9235a5d3dee08805cf964f3a17210
